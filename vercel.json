{
  "version": 2,
  "builds": [
    {
      "src": "api/auth.js",
      "use": "@vercel/node"
    },
    {
      "src": "*.html",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/auth",
      "dest": "/api/auth.js",
      "methods": ["GET"]
    },
    {
      "src": "/api/auth/callback",
      "dest": "/api/auth.js",
      "methods": ["GET"]
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
